@model List<BymenFinal.Models.commitee>
@{
    ViewBag.Title = "Commitees";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Committees</h4>
                <a class="btn btn-primary" href="/Admin/AddCommitee">Add Committee</a>
                <div class="table-responsive">
                    <table class="table table-striped">
                        <thead>
                            <tr>
                                <th> Committee Name </th>
                                <th> Mode </th>

                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>

                                    <td> @item.comName </td>
                                    <td> @item.comType </td>
                                    <td><a class="btn btn-danger" href="/Admin/DeleteCommite/@item.Id">Delete</a></td>
                                </tr>
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


